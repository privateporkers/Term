@{
    ViewData["Title"] = "Index";
}
<head>
    <script src="~/Scripts/p5.js" type="text/javascript"></script>
    <script src="~/Scripts/p5.dom.js" type="text/javascript"></script>
</head>

<body>
    <script type="text/javascript">
        var input;
        var row1;
        var table1;

        function setup() {
            noCanvas();
            table1 = createElement('table');
            row1 = createElement('tr');
            row1.parent(table1);
            row1.html('Test');

            input = createInput();

            input.changed(change);
        }

        function change() {
            var row = createElement('tr');
            row.parent(table1);
            row.html(input.value());
        }
        function draw() {

        }
    </script>
</body>
